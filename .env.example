# TCMB MCP Pro - Environment Variables
# Copy this file to .env and customize as needed

# Cache Settings
TCMB_CACHE_ENABLED=true
TCMB_CACHE_DB_PATH=tcmb_cache.db
TCMB_CACHE_TTL_TODAY=3600
TCMB_CACHE_TTL_HISTORICAL=31536000

# API Settings
TCMB_TIMEOUT=10
TCMB_MAX_RETRIES=3
TCMB_RETRY_DELAY=1.0
TCMB_REQUEST_DELAY=0.1

# General Settings
TCMB_DEBUG=false
TCMB_LOG_LEVEL=INFO
